.biscuits-wrap
    padding: 15px
    border-bottom: 1px solid #e9e9e9
    &:hover
        background: #F8F8F8
    &:last-child
        border-bottom: 0
    .biscuits-left
        display: inline-block
        background: url('../../../images/kitty_265x244_2x.png') no-repeat
        background-size: 80px 80px
        width: 80px
        height: 80px
        img
            width: 100%
            height: 100%
        &:hover
            cursor: pointer

    .biscuits-right
        display: inline-block
        padding: 0 0 0 15px
        .title
            font-size: 20px
            font-weight: 300
            line-height: 24px
            &:hover
                cursor: pointer
        .text
            display: block
            font-size: 13px
            font-weight: 400
            line-height: 33px
            color: #999
            overflow: hidden
            text-overflow: ellipsis
            white-space: nowrap

