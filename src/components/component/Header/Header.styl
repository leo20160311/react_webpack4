.header
    width: 100%
    height: 70px
    line-height: 70px
    background: #fff
    border-bottom: 1px solid #e8e8e8
    .header-wrap
        max-width: 1100px
        margin: 0 auto
        padding: 0 15px
        .logo
            display: inline-block
            width: 33px
            height: 33px
            background:url(../../../images/logo.svg) no-repeat
            vertical-align: middle
        .txt
            display: inline-block
            font-size: 22px
            color: #333333; 
            margin:0 24px 0 20px
            vertical-align: middle
